<div class="row mt-5">
  <div class="col-md-11 col-sm-10 col-12 mb-3">
    <h3>自主トレメニュー一覧</h3>
  </div>
  <div class="col-md-1 col-2 px-0 ml-auto">
    <%= link_to new_therapist_menu_path do %>
      <div class="new-btn">
        <i class="fas fa-plus"></i>
      </div>
    <% end %>
  </div>
</div>
<div class="col-lg-12 mx-auto">
  <div class="row">
    <div class="form-group ml-auto">
      <%= form_with url: therapist_serarch_path, method: :get, local: false do |f| %>
        <%= f.label :genre_name, "ジャンル名" %>
        <%= f.text_field :genre_name, placeholder: "ジャンル名を入力" %>
        <%= f.submit "検索" %>
      <% end %>
    </div>
  </div>
</div>
<div id="search_error"></div>
<div id="table">
  <%= render "table", menus: @menus %>
</div>
<div class="d-flex justify-content-center" id="paginate">
  <%= paginate @menus %>
</div>